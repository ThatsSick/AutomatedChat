function randomMessage()
    local ChatMessages = {
        "ğŸ”´ğŸŸ¢ Want the cheapest jailbreak items? | ,gg /ambrosol | ğŸŸ¢ğŸ”´",
        "ğŸ›‘ Want a $20 torp? | ,gg /ambrosol | ğŸ›‘",
        "ğŸŸ¢ Tired of overpricers? | ,gg /ambrosol | ğŸŸ¢",
        
        
    }

    local TextChatService = game:GetService("TextChatService")
    local targetChannel = TextChatService.ChatInputBarConfiguration.TargetTextChannel

    local function sendRandomMessage()
        local message = ChatMessages[math.random(1, #ChatMessages)]
        targetChannel:SendAsync(message, "All")
    end

    -- Sleep nigga
    while true do
        sendRandomMessage()
        wait(5)
    end
end
